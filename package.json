    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "prettier": "1.16.1"
