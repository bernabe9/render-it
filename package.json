{
  "name": "render-it",
  "version": "0.0.13",
  "description": "Crawl and render any javascript web app to create static sites ready for SEO",
  "main": "src/index.js",
  "scripts": {
    "start": "node bin/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write './**/*.js'",
